{
  "name": "cuhawk-mean",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many -t build",
    "build:dev:api": "nx build api",
    "build:dev:client": "nx build client",
    "build:prod": "nx run-many -t build --all -c production",
    "build:prod:api": "nx build -t api -c production",
    "build:prod:client": "nx build client:build:production",
    "start:dev": "nx run-many -t serve --all",
    "start:dev:api": "nx run api:serve:development",
    "start:dev:client": "nx run client:serve:development",
    "test:dev:api": "nx test api",
    "test:dev:client": "nx test client",
    "e2e:api": "concurrently --kill-others-on-fail \"yarn start:dev:api\" \"nx e2e api-e2e\"",
    "e2e:client": "concurrently --kill-others-on-fail \"yarn start:dev:client\" \"nx e2e client-e2e\"",
    "graph": "nx dep-graph"
  },
  "private": true,
  "dependencies": {
